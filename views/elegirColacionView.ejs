    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/resources/css/colacion.css">
    <script defer src="/resources/js/colacion.js"></script>
    <script defer src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>
</head>

<body>
    <% if (deshabilitado) { %>
        <h1>Ya solicitaste una colacion maldito insecto</h1>
        <a href="<%= RutaTipoFuncionario %>"><button id="button1">Ir</button></a>
        <a href="/form-confirmar-colacion"><button id="button1">Confirmar colacion</button></a>
    <% } else { %>
        <form id="tipoRegimenForm" method="post" action="/auth/agregar-colacion">
            <input type="hidden" name="regimen" id="regimenInput" value="">
            <div id="tiposRegimenList"></div>
        </form>
    <% } %>
</body>

</html>